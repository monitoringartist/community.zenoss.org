    <item>
      <title>RE: HP ProLiant Monitor</title>
      <link>http://community.zenoss.org/docs/DOC-3394#comments-2856</link>
      <description>&lt;!-- [DocumentBodyStart:83c26ad5-24dd-464e-8489-7e00ede37735] --&gt;&lt;div class="jive-rendered-content"&gt;&lt;p&gt;Hi Egor,&lt;/p&gt;&lt;p style="min-height: 8pt; height: 8pt; padding: 0px;"&gt;&amp;#160;&lt;/p&gt;&lt;p&gt;I already create some thresholds that inform of errors occurring on hard disk (notreadyerr, hardwareerror, hardreaderrs &amp;amp; hatdwriteerrs, etc.), also create the transforms to produce a more readable output. It should be a cool feature for your zenpack.&lt;/p&gt;&lt;p style="min-height: 8pt; height: 8pt; padding: 0px;"&gt;&amp;#160;&lt;/p&gt;&lt;p&gt;##########################################################################################&lt;/p&gt;&lt;p&gt;import re&lt;/p&gt;&lt;p&gt;match = re.search('threshold of diskerror [^:]+: current value ([\d\.]+)', evt.message)&lt;/p&gt;&lt;p&gt;if match and device:&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160; evt.summary = "Hard Disk with Hardware or Not Ready Erros: Future Replacement Posible"&lt;/p&gt;&lt;p style="min-height: 8pt; height: 8pt; padding: 0px;"&gt;&amp;#160;&lt;/p&gt;&lt;p&gt;import re&lt;/p&gt;&lt;p&gt;match = re.search('threshold of harderror [^:]+: current value ([\d\.]+)', evt.message)&lt;/p&gt;&lt;p&gt;if match and device:&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160; evt.summary = "Hard Disk with Write or Read Erros: Future Replacement Posible"&lt;/p&gt;&lt;p&gt;##########################################################################################&lt;/p&gt;&lt;p&gt;import re&lt;/p&gt;&lt;p&gt;match = re.search('threshold of cpqDaPhyDrv status', evt.message)&lt;/p&gt;&lt;p&gt;if match and device:&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160; if evt.compStatus == 3.0:&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.summary = "Hard Disk Failure: Replace Failed Hard Disk"&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.severity = 5&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160; elif evt.compStatus == 4.0:&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.summary = "Hard Disk Predictive Failure: Check Disk Errors"&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.severity = 4&lt;/p&gt;&lt;p&gt;##########################################################################################&lt;/p&gt;&lt;p&gt;import re&lt;/p&gt;&lt;p&gt;match = re.search('threshold of cpqDaLogDrv status', evt.message)&lt;/p&gt;&lt;p&gt;if match and device:&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160; if evt.compStatus == 2:&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.summary = "Logical Volume Status: Volume OK"&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.severity = 2&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160; elif evt.compStatus == 3:&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.summary = "Logical Volume Status: Volume Failed"&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.severity = 5&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160; elif evt.compStatus == 4:&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.summary = "Logical Volume Status: Volume UN-configured"&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.severity = 2&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160; elif evt.compStatus == 5:&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.summary = "Logical Volume Status: Volume Recovering"&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.severity = 3&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160; elif evt.compStatus == 6:&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.summary = "Logical Volume Status: Volume Ready Rebuild"&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.severity = 3&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160; elif evt.compStatus == 7:&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.summary = "Logical Volume Status: Volume Rebuilding"&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.severity = 3&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160; else:&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; evt.summary = "Logical Volume Status: Unknown"&lt;/p&gt;&lt;/div&gt;&lt;!-- [DocumentBodyEnd:83c26ad5-24dd-464e-8489-7e00ede37735] --&gt;</description>
