icTime = "20110208095428.697000+060";&lt;br/&gt;&amp;#160;&amp;#160;&amp;#160; StatisticTime = "20110208105613.489000+060";&lt;br/&gt;&amp;#160;&amp;#160;&amp;#160; SampleInterval = "00000000000000.000000:000";&lt;br/&gt;&amp;#160;&amp;#160;&amp;#160; ElementType = 2;&lt;br/&gt;&amp;#160;&amp;#160;&amp;#160; TotalIOs = 10209701;&lt;br/&gt;&amp;#160;&amp;#160;&amp;#160; KBytesTransferred = 181979705;&lt;br/&gt;};&lt;/p&gt;&lt;p style="min-height: 8pt; height: 8pt; padding: 0px;"&gt;&amp;#160;&lt;/p&gt;&lt;p&gt;okay, so now i know there is an hpeva_storagesystem and hpeva_storagesystemstatisticaldata classes available.&lt;/p&gt;&lt;p style="min-height: 8pt; height: 8pt; padding: 0px;"&gt;&amp;#160;&lt;/p&gt;&lt;p&gt;doing the same for HostFCPorts:&lt;/p&gt;&lt;p style="min-height: 8pt; height: 8pt; padding: 0px;"&gt;&amp;#160;&lt;/p&gt;&lt;p&gt;&lt;strong&gt;&lt;span&gt;C:\Program Files\Hewlett-Packard\SMI-S\CXWSCimom\bin&amp;gt;cxmofd.bat -h&amp;#160; &lt;/span&gt;&lt;a class="jive-link-external-small" href="http://localhost"&gt;http://localhost&lt;/a&gt;&lt;span&gt; -l &amp;lt;user:pass&amp;gt; -n root/eva -i hpeva&lt;/span&gt;&lt;strong&gt;_&lt;/strong&gt;&lt;/strong&gt;&lt;strong&gt;hostfcport&lt;/strong&gt;&lt;/p&gt;&lt;p style="min-height: 8pt; height: 8pt; padding: 0px;"&gt;&amp;#160;&lt;/p&gt;&lt;p&gt;produces:&lt;/p&gt;&lt;p style="min-height: 8pt; height: 8pt; padding: 0px;"&gt;&amp;#160;&lt;/p&gt;&lt;p&gt;// enumerating "root\\eva:hpeva_hostfcport"&lt;br/&gt;// 49525 bytes&lt;br/&gt;instance of HPEVA_HostFCPort as $hpeva_hostfcport0&lt;br/&gt;{&lt;br/&gt;&amp;#160;&amp;#160;&amp;#160; Caption = "hostport1";&lt;br/&gt;&amp;#160;&amp;#160;&amp;#160; ElementName = "\\Hardware\\Rack 1\\Controller Enclosure 5\\Controller A.hostport1";&lt;br/&gt;&amp;#160;&amp;#160;&amp;#160; Name = "50001FE1500F4228";&lt;br/&gt;&amp;#160;&amp;#160;&amp;#160; OperationalStatus = { 2 };&lt;br/&gt;&amp;#160;&amp;#160;&amp;#160; Status = "good";&lt;br/&gt;&amp;#160;&amp;#160;&amp;#1