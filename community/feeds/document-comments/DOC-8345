-1.2.1-py2.6.egg/ZenPacks/community/Clariion/libexec/perfs.pl&amp;#160;&amp;#160;&amp;#160; 2011-09-29 12:54:27.000000000 -0400&lt;/p&gt;&lt;p&gt;***************&lt;/p&gt;&lt;p&gt;*** 34,40 ****&lt;/p&gt;&lt;p&gt;&amp;#160; {&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; # get disk&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; die("\n") unless($id);&lt;/p&gt;&lt;p&gt;!&amp;#160;&amp;#160;&amp;#160;&amp;#160; open(FIC, "$navicli -Timeout 20 -user $user -password $pass -scope 0 -address $ip getdisk $id 2&amp;gt;/dev/null|") or die("Unknown|results\n");&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; my @lines = &amp;lt;FIC&amp;gt;;&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; die("Unknown|results\n") unless(@lines);&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; die("Unknown|No Response\n") unless($lines[0] !~ m/^Could not connect to the specified host/);&lt;/p&gt;&lt;p&gt;--- 34,41 ----&lt;/p&gt;&lt;p&gt;&amp;#160; {&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; # get disk&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; die("\n") unless($id);&lt;/p&gt;&lt;p&gt;!&amp;#160;&amp;#160;&amp;#160;&amp;#160; open(FIC, "$navicli -Timeout 20 -User $user -Password $pass -Scope 0 -Address $ip getdisk $id 2&amp;gt;/dev/null|") or die("Unknown|results\n");&lt;/p&gt;&lt;p&gt;! &lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; my @lines = &amp;lt;FIC&amp;gt;;&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; die("Unknown|results\n") unless(@lines);&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&amp;#160;&amp;#160;&amp;#160; die("Unknown|No Response\n") unless($lines[0] !~ m/^Could not connect to the specified host/);&lt;/p&gt;&lt;p&gt;***************&lt;/p&gt;&lt;p&gt;*** 101,107 ****&lt;/p&gt;&lt;p&gt;&amp;#160; sub checkLun&lt;/p&gt;&lt;p&gt;&amp;#160; {&lt;/p&gt;&lt;p&gt;&amp;#160;&amp;#160;&am